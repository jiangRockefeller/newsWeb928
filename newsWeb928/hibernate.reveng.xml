<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="comments" catalog="newsystem"
		class="com.newsWeb928.pojo.Comments">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="news_test" catalog="newsystem"
		class="com.newsWeb928.pojo.Users">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="news" catalog="newsystem"
		class="com.newsWeb928.pojo.News">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="topic" catalog="newsystem"
	class="com.newsWeb928.pojo.Topic">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>